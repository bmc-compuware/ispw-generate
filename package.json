{
  "name": "code-pipeline-generate",
  "version": "2.0.0",
  "description": "GitHub action to execute a BMC AMI DevX Workbench Code Pipeline generate",
  "publisher": "BMC",
  "main": "./dist/index.js",
  "scripts": {
    "build": "ncc build index.js -o dist --source-map --license licenses.txt",
    "lint": "npx eslint index.js test/**/*.js --fix",
    "lint:check": "npx eslint index.js test/**/*.js",
    "test": "nyc --all=true --exclude=\"dist/**\" --exclude=\"coverage/**\" --exclude=\"test/**\" --reporter=json --reporter=text mocha",
    "test:watch": "mocha --watch",
    "coverage": "nyc check-coverage --lines 65 --branches 71 --statements 65 --functions 75",
    "coverage:report": "nyc report --reporter=html",
    "check": "npm run lint:check && npm run build && npm run test && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmc-compuware/ispw-generate.git"
  },
  "keywords": [
    "ISPW",
    "Compuware",
    "BMC",
    "AMI",
    "DevX",
    "generate",
    "code-pipeline"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmc-compuware/ispw-generate/issues"
  },
  "homepage": "https://github.com/bmc-compuware/ispw-generate#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^4.0.0",
    "@bmc-compuware/ispw-action-utilities": "https://github.com/bmc-compuware/ispw-action-utilities.git#main",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nyc": "^15.1.0"
  }
}
